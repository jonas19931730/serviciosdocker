package com.nirsa.nw.web.app.servicioatunera.model.dto;

import java.io.Serializable;
import java.util.List;
import java.util.Map;

public class RequestMonitor implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Map<String, Object> jsonCabecera;
	private List<Detalle> detalle;

	public RequestMonitor() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Map<String, Object> getJsonCabecera() {
		return jsonCabecera;
	}

	public void setJsonCabecera(Map<String, Object> jsonCabecera) {
		this.jsonCabecera = jsonCabecera;
	}

	public List<Detalle> getDetalle() {
		return detalle;
	}

	public void setDetalle(List<Detalle> detalle) {
		this.detalle = detalle;
	}

}

class Detalle implements Serializable {
	
	private static final long serialVersionUID = 1L;
	private Integer idposicion;
	private Map<String, Object> jsonValores;

	public Detalle() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Integer getIdposicion() {
		return idposicion;
	}

	public void setIdnotificacion(Integer idposicion) {
		this.idposicion = idposicion;
	}

	public Map<String, Object> getJsonValores() {
		return jsonValores;
	}

	public void setJsonValores(Map<String, Object> jsonValores) {
		this.jsonValores = jsonValores;
	}

}
