spring:
  cloud:
    gateway:
      routes:
      - id: servicio-atunera
        uri: lb://servicio-atunera
        predicates:          - Path=/api/atunera/**
        filters:
          - StripPrefix=2
      - id: servicio-auth-local
        uri: lb://servicio-auth-local
        predicates:
          - Path=/api/authlocal/**
        filters:
          - StripPrefix=2
